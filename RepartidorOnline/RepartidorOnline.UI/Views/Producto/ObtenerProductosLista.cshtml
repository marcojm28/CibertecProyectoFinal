@model List<RepartidorOnline.Domain.Products.Producto>

@foreach (var item in Model)
{

    <tr>

        <td>@item.NombreProducto</td>
        <td>@item.Precio</td>
        <td>@item.Stock</td>
        <td>
            <div class="btn-group col-12">
                <button type="button" class="btnEditarProducto btn btn-warning" value="@item.IdProducto">Editar <i class="fas fa-edit"></i></button>
                <button type="button" class="btnEliminarProducto btn btn-danger" value="@item.IdProducto">Eliminar <i class="fas fa-trash-alt"></i></button>
            </div>

        </td>
    </tr>

}

<script type="text/javascript">
        $(document).ready(app.pagina.Producto.InitProducto);
</script>
